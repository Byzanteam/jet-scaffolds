{
  "name": "vue-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .json,.js,.vue src",
    "lint:fix": "eslint --ext .json,.js,.vue --fix src",
    "clone:git-actions": "degit Byzanteam/jet-scaffolds/git-actions .github/workflows",
    "clone:deploy": "degit Byzanteam/jet-scaffolds/deploy deploy",
    "prepare": "pnpm clone:deploy && pnpm clone:git-actions && husky install",
    "precommit": "lint-staged",
    "commitlint": "commitlint"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "prettier": "@byzanteam/prettier-config",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@byzanteam/commitlint-config": "^1.0.0",
    "@byzanteam/eslint-config-vue": "^4.0.0",
    "@byzanteam/prettier-config": "^1.0.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "degit": "^2.8.4",
    "eslint": "^8.26.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "vite": "^3.2.0"
  }
}
