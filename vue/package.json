{
  "engineStrict": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "vue-tsc --noEmit && eslint --ext .js,.ts,.vue src",
    "lint:fix": "vue-tsc --noEmit && eslint --ext .js,.ts,.vue --fix src",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix"
  },
  "dependencies": {
    "vue": "^3.2.41",
    // @if router
    "vue-router": "^4.1.6"
    // @endif
  },
  "devDependencies": {
    "@byzanteam/eslint-config-vue": "^4.0.0",
    "@byzanteam/eslint-config-vue-ts": "^1.0.0",
    "@byzanteam/tsconfig": "^1.0.0",
    "@types/node": "^18.11.8",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.26.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.0",
    "vue-tsc": "^1.0.9"
  }
}
